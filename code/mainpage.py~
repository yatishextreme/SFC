        ui.cp10g7vd4_time.setEnabled(True)
        ui.cp10g7vd4_line.setText("000")
        ui.cp10g7vd4_line.setEnabled(False)
            
        ui.cp10g7vd5_label.setText("VD5 (HH:MM)")
        ui.cp10g7vd5_time.setEnabled(True)
        ui.cp10g7vd5_line.setText("000")
        ui.cp10g7vd5_line.setEnabled(False)
    else:
        #for qty
        ui.cp10g7vd1_label.setText("VD1 (l/h)")
        ui.cp10g7vd1_time.setTime(QtCore.QTime(00, 00,))
        ui.cp10g7vd1_time.setEnabled(False)
        ui.cp10g7vd1_line.setEnabled(True)
        ui.cp10g7vd1_line.clear()
            
        ui.cp10g7vd2_label.setText("VD2 (l/h)")
        ui.cp10g7vd2_time.setTime(QtCore.QTime(00, 00,))
        ui.cp10g7vd2_time.setEnabled(False)
        ui.cp10g7vd2_line.setEnabled(True)
        ui.cp10g7vd2_line.clear()
            
        ui.cp10g7vd3_label.setText("VD3 (l/h)")
        ui.cp10g7vd3_time.setTime(QtCore.QTime(00, 00,))
        ui.cp10g7vd3_time.setEnabled(False)
        ui.cp10g7vd3_line.setEnabled(True)
        ui.cp10g7vd3_line.clear()
            
        ui.cp10g7vd4_label.setText("VD4 (l/h)")
        ui.cp10g7vd4_time.setEnabled(False)
        ui.cp10g7vd4_line.setEnabled(True)
        ui.cp10g7vd4_line.clear()
            
