""" update functions"""
def updateirrigationcp01g1(measure):
    if measure ==0:
        #active for time
        ui.cp01g1qtystarttime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g1totalqty_line.setText("000000")
        ui.cp01g1preqty_line.setText("000000")
        ui.cp01g1postqty_line.setText("000000")
        ui.cp01g1qtystarttime_label.setEnabled(False)
        ui.cp01g1qtystarttime_time.setEnabled(False)
        ui.cp01g1totalqty_label.setEnabled(False)
        ui.cp01g1totalqty_line.setEnabled(False)
        ui.cp01g1preqty_label.setEnabled(False)
        ui.cp01g1preqty_line.setEnabled(False)
        ui.cp01g1postqty_label.setEnabled(False)
        ui.cp01g1postqty_line.setEnabled(False)
            
        ui.cp01g1starttime_label.setEnabled(True)
        ui.cp01g1starttime_time.setEnabled(True)
        ui.cp01g1stoptime_label.setEnabled(True)
        ui.cp01g1stoptime_time.setEnabled(True)
        ui.cp01g1pretime_label.setEnabled(True)
        ui.cp01g1pretime_time.setEnabled(True)
        ui.cp01g1posttime_label.setEnabled(True)
        ui.cp01g1posttime_time.setEnabled(True)
    else:
        #active for qty
        ui.cp01g1totalqty_line.clear()
        ui.cp01g1preqty_line.clear()
        ui.cp01g1postqty_line.clear()
        ui.cp01g1qtystarttime_label.setEnabled(True)
        ui.cp01g1qtystarttime_time.setEnabled(True)
        ui.cp01g1totalqty_label.setEnabled(True)
        ui.cp01g1totalqty_line.setEnabled(True)
        ui.cp01g1preqty_label.setEnabled(True)
        ui.cp01g1preqty_line.setEnabled(True)
        ui.cp01g1postqty_label.setEnabled(True)
        ui.cp01g1postqty_line.setEnabled(True)
        
        ui.cp01g1starttime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g1stoptime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g1pretime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g1posttime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g1starttime_label.setEnabled(False)
        ui.cp01g1starttime_time.setEnabled(False)
        ui.cp01g1stoptime_label.setEnabled(False)
        ui.cp01g1stoptime_time.setEnabled(False)
        ui.cp01g1pretime_label.setEnabled(False)
        ui.cp01g1pretime_time.setEnabled(False)
        ui.cp01g1posttime_label.setEnabled(False)
        ui.cp01g1posttime_time.setEnabled(False)
def updateventuricp01g1(measure):
    if measure ==0:
        #for time
        ui.cp01g1vd1_label.setText("VD1 (HH:MM)")
        ui.cp01g1vd1_time.setEnabled(True)
        ui.cp01g1vd1_line.setText("000")
        ui.cp01g1vd1_line.setEnabled(False)
            
        ui.cp01g1vd2_label.setText("VD2 (HH:MM)")
        ui.cp01g1vd2_time.setEnabled(True)
        ui.cp01g1vd2_line.setText("000")
        ui.cp01g1vd2_line.setEnabled(False)
            
        ui.cp01g1vd3_label.setText("VD3 (HH:MM)")
        ui.cp01g1vd3_time.setEnabled(True)
        ui.cp01g1vd3_line.setText("000")
        ui.cp01g1vd3_line.setEnabled(False)
            
        ui.cp01g1vd4_label.setText("VD4 (HH:MM)")
        ui.cp01g1vd4_time.setEnabled(True)
        ui.cp01g1vd4_line.setText("000")
        ui.cp01g1vd4_line.setEnabled(False)
            
        ui.cp01g1vd5_label.setText("VD5 (HH:MM)")
        ui.cp01g1vd5_time.setEnabled(True)
        ui.cp01g1vd5_line.setText("000")
        ui.cp01g1vd5_line.setEnabled(False)
    else:
        #for qty
        ui.cp01g1vd1_label.setText("VD1 (l/h)")
        ui.cp01g1vd1_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g1vd1_time.setEnabled(False)
        ui.cp01g1vd1_line.setEnabled(True)
        ui.cp01g1vd1_line.clear()
            
        ui.cp01g1vd2_label.setText("VD2 (l/h)")
        ui.cp01g1vd2_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g1vd2_time.setEnabled(False)
        ui.cp01g1vd2_line.setEnabled(True)
        ui.cp01g1vd2_line.clear()
            
        ui.cp01g1vd3_label.setText("VD3 (l/h)")
        ui.cp01g1vd3_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g1vd3_time.setEnabled(False)
        ui.cp01g1vd3_line.setEnabled(True)
        ui.cp01g1vd3_line.clear()
            
        ui.cp01g1vd4_label.setText("VD4 (l/h)")
        ui.cp01g1vd4_time.setEnabled(False)
        ui.cp01g1vd4_line.setEnabled(True)
        ui.cp01g1vd4_line.clear()
            
        ui.cp01g1vd5_label.setText("VD5 (l/h)")
        ui.cp01g1vd5_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g1vd5_time.setEnabled(False)
        ui.cp01g1vd5_line.setEnabled(True)
        ui.cp01g1vd5_line.clear()
def updatephcp01g1(k):
    if k==True:
        ui.cp01g1ph_combo.setEnabled(True)
        ui.cp01g1ph_combo.setCurrentIndex(0)
    else:
        ui.cp01g1ph_combo.setCurrentIndex(0)
        ui.cp01g1ph_combo.setEnabled(False)
def updateirrigationcp01g2(measure):
    if measure ==0:
        #active for time
        ui.cp01g2qtystarttime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g2totalqty_line.setText("000000")
        ui.cp01g2preqty_line.setText("000000")
        ui.cp01g2postqty_line.setText("000000")
        ui.cp01g2qtystarttime_label.setEnabled(False)
        ui.cp01g2qtystarttime_time.setEnabled(False)
        ui.cp01g2totalqty_label.setEnabled(False)
        ui.cp01g2totalqty_line.setEnabled(False)
        ui.cp01g2preqty_label.setEnabled(False)
        ui.cp01g2preqty_line.setEnabled(False)
        ui.cp01g2postqty_label.setEnabled(False)
        ui.cp01g2postqty_line.setEnabled(False)
            
        ui.cp01g2starttime_label.setEnabled(True)
        ui.cp01g2starttime_time.setEnabled(True)
        ui.cp01g2stoptime_label.setEnabled(True)
        ui.cp01g2stoptime_time.setEnabled(True)
        ui.cp01g2pretime_label.setEnabled(True)
        ui.cp01g2pretime_time.setEnabled(True)
        ui.cp01g2posttime_label.setEnabled(True)
        ui.cp01g2posttime_time.setEnabled(True)
    else:
        #active for qty
        ui.cp01g2totalqty_line.clear()
        ui.cp01g2preqty_line.clear()
        ui.cp01g2postqty_line.clear()
        ui.cp01g2qtystarttime_label.setEnabled(True)
        ui.cp01g2qtystarttime_time.setEnabled(True)
        ui.cp01g2totalqty_label.setEnabled(True)
        ui.cp01g2totalqty_line.setEnabled(True)
        ui.cp01g2preqty_label.setEnabled(True)
        ui.cp01g2preqty_line.setEnabled(True)
        ui.cp01g2postqty_label.setEnabled(True)
        ui.cp01g2postqty_line.setEnabled(True)
        
        ui.cp01g2starttime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g2stoptime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g2pretime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g2posttime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp01g2starttime_label.setEnabled(False)
        ui.cp01g2starttime_time.setEnabled(False)
        ui.cp01g2stoptime_label.setEnabled(False)
        ui.cp01g2stoptime_time.setEnabled(False)
        ui.cp01g2pretime_label.setEnabled(False)
        ui.cp01g2pretime_time.setEnabled(False)
        ui.cp01g2posttime_label.setEnabled(False)
        ui.cp01g2posttime_time.setEnabled(False)
