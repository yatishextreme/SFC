def updateirrigationcp10g8(measure):
    if measure ==0:
        #active for time
        ui.cp10g8qtystarttime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp10g8totalqty_line.setText("000000")
        ui.cp10g8preqty_line.setText("000000")
        ui.cp10g8postqty_line.setText("000000")
        ui.cp10g8qtystarttime_label.setEnabled(False)
        ui.cp10g8qtystarttime_time.setEnabled(False)
        ui.cp10g8totalqty_label.setEnabled(False)
        ui.cp10g8totalqty_line.setEnabled(False)
        ui.cp10g8preqty_label.setEnabled(False)
        ui.cp10g8preqty_line.setEnabled(False)
        ui.cp10g8postqty_label.setEnabled(False)
        ui.cp10g8postqty_line.setEnabled(False)
            
        ui.cp10g8starttime_label.setEnabled(True)
        ui.cp10g8starttime_time.setEnabled(True)
        ui.cp10g8stoptime_label.setEnabled(True)
        ui.cp10g8stoptime_time.setEnabled(True)
        ui.cp10g8pretime_label.setEnabled(True)
        ui.cp10g8pretime_time.setEnabled(True)
        ui.cp10g8posttime_label.setEnabled(True)
        ui.cp10g8posttime_time.setEnabled(True)
    else:
        #active for qty
        ui.cp10g8totalqty_line.clear()
        ui.cp10g8preqty_line.clear()
        ui.cp10g8postqty_line.clear()
        ui.cp10g8qtystarttime_label.setEnabled(True)
        ui.cp10g8qtystarttime_time.setEnabled(True)
        ui.cp10g8totalqty_label.setEnabled(True)
        ui.cp10g8totalqty_line.setEnabled(True)
        ui.cp10g8preqty_label.setEnabled(True)
        ui.cp10g8preqty_line.setEnabled(True)
        ui.cp10g8postqty_label.setEnabled(True)
        ui.cp10g8postqty_line.setEnabled(True)
        
        ui.cp10g8starttime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp10g8stoptime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp10g8pretime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp10g8posttime_time.setTime(QtCore.QTime(00, 00,))
        ui.cp10g8starttime_label.setEnabled(False)
        ui.cp10g8starttime_time.setEnabled(False)
        ui.cp10g8stoptime_label.setEnabled(False)
        ui.cp10g8stoptime_time.setEnabled(False)
        ui.cp10g8pretime_label.setEnabled(False)
        ui.cp10g8pretime_time.setEnabled(False)
        ui.cp10g8posttime_label.setEnabled(False)
        ui.cp10g8posttime_time.setEnabled(False)
