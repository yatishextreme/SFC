        ui.cp10g7posttime_label.setEnabled(False)
        ui.cp10g7posttime_time.setEnabled(False)
def updateventuricp10g7(measure):
    if measure ==0:
        #for time
        ui.cp10g7vd1_label.setText("VD1 (HH:MM)")
        ui.cp10g7vd1_time.setEnabled(True)
        ui.cp10g7vd1_line.setText("000")
        ui.cp10g7vd1_line.setEnabled(False)
            
        ui.cp10g7vd2_label.setText("VD2 (HH:MM)")
        ui.cp10g7vd2_time.setEnabled(True)
        ui.cp10g7vd2_line.setText("000")
        ui.cp10g7vd2_line.setEnabled(False)
            
        ui.cp10g7vd3_label.setText("VD3 (HH:MM)")
        ui.cp10g7vd3_time.setEnabled(True)
        ui.cp10g7vd3_line.setText("000")
        ui.cp10g7vd3_line.setEnabled(False)
            
