def clearbuttoncp01g1():
    ui.cp01g1groupname_line.clear();ui.cp01g1irrmeasure_combo.setCurrentIndex(0);ui.cp01g1starttime_time.setTime(QtCore.QTime(00,00,00))
    ui.cp01g1stoptime_time.setTime(QtCore.QTime(00,00,00))
    ui.cp01g1pretime_time.setTime(QtCore.QTime(00,00,00))
    ui.cp01g1posttime_time.setTime(QtCore.QTime(00,00,00))
