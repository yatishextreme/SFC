"""    loadfunctions"""
def loadbuttoncp01g1():
    print "loadng cp01g1"
    os.chdir(PATHNAME)
    filter="Group Files (*.txt)"
    filename=QtGui.QFileDialog.getOpenFileName(MainWindow,"LOAD THE GROUP",PATHNAME,filter)
    f=open(filename,'rb')
    x=pickle.load(f)
    print x
    ui.cp01g1groupname_line.setText(x[0])
    ui.cp01g1irrmeasure_combo.setCurrentIndex(x[1])
    ui.cp01g1starttime_time.setTime(x[2])
    ui.cp01g1stoptime_time.setTime(x[3])
    ui.cp01g1pretime_time.setTime(x[4])
    ui.cp01g1posttime_time.setTime(x[5])
    ui.cp01g1qtystarttime_time.setTime(x[6])
    ui.cp01g1totalqty_line.setText(x[7])
    ui.cp01g1preqty_line.setText(x[8])
    ui.cp01g1postqty_line.setText(x[9])
    ui.cp01g1vdmeasure_combo.setCurrentIndex(x[10])
    ui.cp01g1vd1_time.setTime(x[11])
    ui.cp01g1vd1_line.setText(x[12])
    ui.cp01g1vd1_s1.setChecked(x[13])
    ui.cp01g1vd1_s2.setChecked(x[14])
    ui.cp01g1vd1_s3.setChecked(x[15])
    ui.cp01g1vd1_s4.setChecked(x[16])
    ui.cp01g1vd2_time.setTime(x[17])
    ui.cp01g1vd2_line.setText(x[18])
    ui.cp01g1vd2_s1.setChecked(x[19])
    ui.cp01g1vd2_s2.setChecked(x[20])
    ui.cp01g1vd2_s3.setChecked(x[21])
    ui.cp01g1vd2_s4.setChecked(x[22])
    ui.cp01g1vd3_time.setTime(x[23])
    ui.cp01g1vd3_line.setText(x[24])
    ui.cp01g1vd2_s1.setChecked(x[25])
    ui.cp01g1vd2_s1.setChecked(x[26])
    ui.cp01g1vd2_s1.setChecked(x[27])
    ui.cp01g1vd2_s1.setChecked(x[28])
    ui.cp01g1vd4_time.setTime(x[29])
    ui.cp01g1vd4_line.setText(x[30])
    ui.cp01g1vd2_s1.setChecked(x[31])
    ui.cp01g1vd2_s1.setChecked(x[32])
    ui.cp01g1vd2_s1.setChecked(x[33])
    ui.cp01g1vd2_s1.setChecked(x[34])
    ui.cp01g1vd5_time.setTime(x[35])
    ui.cp01g1vd5_line.setText(x[36])
