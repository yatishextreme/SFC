        ui.cp01g3stoptime_label.setEnabled(True)
        ui.cp01g3stoptime_time.setEnabled(True)
        ui.cp01g3pretime_label.setEnabled(True)
        ui.cp01g3pretime_time.setEnabled(True)
        ui.cp01g3posttime_label.setEnabled(True)
        ui.cp01g3posttime_time.setEnabled(True)
    else:
